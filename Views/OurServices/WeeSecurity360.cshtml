<!--Software Start-->
<section class="all-services">
    <div class="container">
        <div class="row">
            <div class="col-xl-4 col-lg-4">
                <div class="pq-widget">
                    <div class="pq-widget pq-widget-port p-0">
                        <div class="menu-service-menu-container">
                            <ul id="menu-service-menu" class="menu">
                                <li @*class="current-menu-item"*@>
                                    <a href="~/OurServices/WeeCodeIGNITE" aria-current="page" style="text-transform:none;">We(e) CodeIGNITE</a>
                                </li>

                                <li class="current-menu-item">
                                    <a href="~/OurServices/WeeSecurity360" aria-current="page" style="text-transform:none;">We(e) Security360</a>
                                </li>

                                <li>
                                    <a href="~/OurServices/WeeCloudSPHERE" aria-current="page" style="text-transform:none;">We(e) CloudSPHERE</a>
                                </li>

                                <li>
                                    <a href="~/OurServices/WeeNextgenFLOW" aria-current="page" style="text-transform:none;">We(e) NextgenFLOW</a>
                                </li>

                                <li>
                                    <a href="~/OurServices/WeeBlockCENTRAL" aria-current="page" style="text-transform:none;">We(e) BlockCENTRAL</a>
                                </li>
                                <li>
                                    <a href="~/OurServices/WeeDatastAIsis" aria-current="page" style="text-transform:none;">We(e) DatastAIsis</a>
                                </li>
                                <li>
                                    <a href="~/OurServices/WeePraaS" aria-current="page" style="text-transform:none;">We(e) PraaS</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div id="media-image">
                    <div class="pq-form-section pq-style-1" style="position:absolute;padding-left:10px;padding-top:10px;">
                        <span class="pq-form-title" style="font-size: 22px; line-height: 10px; font-weight: 900 ">Have an Idea?</span><br />
                        <span class="pq-form-title" style="font-size: 22px; line-height: 10px; font-weight: 900">We can help</span>
                    </div>
                    <a href="contact-us.html"><img src="~/Content/WebsiteContent/images/callcenter.jpg" alt=""></a>
                </div>
            </div>
            <div class="col-xl-8 col-lg-8 mt-4 mt-lg-0" @*style="padding-right: 149px;"*@>
                <img src="~/Content/WebsiteContent/images/services/security360/cybersecurity.jpg" alt="">
                <h2 class="pq-heading mt-4">
                    Comprehensive Security Solutions

                </h2>
                <p class="pq-description">
                    We(e) not only build the apps, but help your businesses protect it and your sensitive
                    data against cyber attacks.
                </p>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-sm-12">
                        <div class="pq-fancy-box pq-fancybox-1">
                            <div class="pq-fancy-media">
                                <img src="~/Content/WebsiteContent/images/services/security360/risk.png" />
                            </div>
                            <div class="pq-fancybox-info">
                                <h5 class="pq-fancy-box-title">Risk Assessment</h5>
                                <p>
                                    Assess your organization's current
                                    cyber security posture and identify
                                    potential vulnerabilities and risks.
                                </p>
                            </div>
                        </div>
                        <div class="pq-fancy-box pq-fancybox-1 pq-mt-30 mb-4">
                            <div class="pq-fancy-media">
                                <img src="~/Content/WebsiteContent/images/services/security360/compilance.png" />
                            </div>
                            <div class="pq-fancybox-info">
                                <h5 class="pq-fancy-box-title">
                                    Compliance
                                </h5>
                                <p>
                                    Help you comply with industry
                                    standards and regulations such as
                                    HIPAA, PCI DSS, and GDPR.
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-12 mt-4 mt-md-0">
                        <div class="pq-fancy-box pq-fancybox-1 pq-ml-30">
                            <div class="pq-fancy-media">
                                <img src="~/Content/WebsiteContent/images/services/security360/penetration.png" />
                            </div>
                            <div class="pq-fancybox-info">
                                <h5 class="pq-fancy-box-title">Penetration Testing</h5>
                                <p>
                                    Perform simulated attacks to
                                    identify vulnerabilities before they
                                    can be exploited by real attackers.
                                </p>
                            </div>
                        </div>
                        <div class="pq-fancy-box pq-fancybox-1 pq-mt-30 pq-ml-30 mb-4">
                            <div class="pq-fancy-media">
                                <img src="~/Content/WebsiteContent/images/services/security360/incident.png" />
                            </div>
                            <div class="pq-fancybox-info">
                                <h5 class="pq-fancy-box-title">Incident Response</h5>
                                <p>
                                    In the event of a cyber attack, our
                                    team will quickly respond to
                                    minimize damage and restore
                                    normal operations as soon as
                                    possible.
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt-4 border-0">
                    <div class="col-lg-6">
                        <img src="~/Content/WebsiteContent/images/services/security360/cybersecurity1.jpg" alt="">
                    </div>
                    <div class="col-lg-6">
                        <h2 class="pq-about-title mt-4 mt-lg-0">
                            <a href="#">
                                Proactive Approach To
                                Cyber Security
                            </a>
                        </h2>
                        <p class="pq-description mb-0">
                            A proactive approach to cyber security, to provide you with the peace of mind
                            that comes with knowing your sensitive
                            data is safe and secure.
                        </p>

                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-12 mt-4 mt-lg-0">
                <div class="inquiry-form pq-bg-grey">
                    <div class="pq-form-section pq-style-1 ">
                        <span class="pq-form-title" style="font-size:20px;">Call Us Now</span><br />
                        <span class="pq-form-title" style="font-size: 20px; font-weight: 700">(800) 781-6632</span>
                        <h5 class="pq-form-title" style="font-size:24px;line-height:28px;">Request A Free Consultation</h5>
                    </div>
                    @using (Html.BeginForm("WeeSecurity360", "OurServices", FormMethod.Post, new { @class = "pq-applyform" }))
                    {
                        <div class="row">
                            <div class="col-md-12">
                                @*<input type="text" id="first-name" class="pq-bg-white name-field" placeholder="Your Name" required>*@
                                @Html.TextBoxFor(m => m.firstName, new { placeholder = "Your Name", @class = "pq-bg-white name-field", required = "required" })
                            </div>
                            <div class="col-md-12">
                                @*<input type="text" id="e-mail" class="pq-bg-white e-mail-field" placeholder="Email" required>*@
                                @Html.TextBoxFor(m => m.email, new { placeholder = "Email", @class = "pq-bg-white e-mail-field", required = "required" })
                            </div>
                            <div class="col-md-12">
                                @*<input type="text" id="phone-number" class="pq-bg-white phone-number-field" placeholder="Cell Phone" required>*@
                                @Html.TextBoxFor(m => m.phoneNumber, new { placeholder = "Phone", @class = "pq-bg-white phone-number-field", required = "required" })
                            </div>
                            <div class="col-md-12">
                                @*<input type="text" id="subject" class="pq-bg-white subject-field" placeholder="Subject" required>*@
                                @Html.TextBoxFor(m => m.Subject, new { placeholder = "Subject", @class = "pq-bg-white subject-field", required = "required" })
                            </div>
                            <div class="col-md-12">
                                @*<textarea name="your-message" id="message" class="pq-bg-white" cols="10" rows="4" placeholder="Your Message" required></textarea>*@
                                @Html.TextAreaFor(m => m.Message, new { rows = "5", cols = "10", placeholder = "Your Message", @class = "pq-bg-white name-field", required = "required" })
                            </div>
                            <div class="col-md-12">
                                <div class="pq-button-block">
                                    <input class="pq-button-text" id="emailForm" type="submit" value="Send Message" />
                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
</section>
<!--Software End-->
